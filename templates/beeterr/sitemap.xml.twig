<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {% for slug in slugs %}
        <url>
            <loc>{{ url('default', {'slug':slug.slug}) }}</loc>
            <lastmod>{{ slug.modified|date('Y-m-d') }}</lastmod>
        </url>
    {% endfor %}
</urlset>
